<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Title>EZSmartCardClient</Title>
    <Version>1.0.0</Version>
    <Authors>Keytos Security</Authors>
    <Copyright>Keytos Security</Copyright>
    <Description>EZSmartCard client is a package that allows you to automate some management API calls for your EZSmartCard Passwordless CMS</Description>
    <PackageProjectUrl>https://www.keytos.io/passwordless-onboarding.html</PackageProjectUrl>
    <PackageIcon>KeytosK.png</PackageIcon>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <RepositoryUrl>https://github.com/markeytos/EZSmartCardClient</RepositoryUrl>
    <PackageTags>passwordless;smartcard Onboarding;FIDO;EZSmartCard;Keytos</PackageTags>
    <PackageReleaseNotes>Fiirst Release</PackageReleaseNotes>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\..\Desktop\Logos\keytosLogoLong\KeytosK.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
    <None Include="..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
	  <PackageReference Include="Microsoft.ApplicationInsights" Version="2.21.0" />
	  <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="7.0.0" />
	  <PackageReference Include="Microsoft.Extensions.Logging" Version="7.0.0" />
	  <PackageReference Include="Microsoft.Extensions.Logging.ApplicationInsights" Version="2.21.0" />
	  <PackageReference Include="Microsoft.Extensions.Logging.EventLog" Version="7.0.0" />
	  <PackageReference Include="Azure.Identity" Version="1.10.2" />
    <PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="7.0.1" />
    <PackageReference Include="Polly" Version="7.2.4" />
  </ItemGroup>

</Project>
